{
  "author": {
    "name": "@brandonhorst"
  },
  "bugs": {
    "url": "https://github.com/laconalabs/elliptical-datetime/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "elliptical": "^1.0.0-rc10",
    "elliptical-number": "^1.0.0-rc2",
    "lodash": "^4.9.0",
    "moment": "^2.12.0"
  },
  "description": "Elliptical phrases to handle natural language dates and times",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "lolex": "^1.4.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "homepage": "https://github.com/laconalabs/elliptical-datetime",
  "keywords": [
    "date",
    "datetime",
    "elliptical",
    "phrase",
    "time"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "elliptical-datetime",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laconalabs/elliptical-datetime.git"
  },
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p build; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "version": "1.0.0-rc3",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
